{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "components/*": ["components/*"],
      "screens/*": ["screens/*"],
      "contexts/*": ["contexts/*"],
      "hooks": ["hooks/index.ts"],
      "utils/*": ["utils/*"],
      "navigation/*": ["navigation/*"],
      "services/*": ["services/*"],
      "config": ["config/index.ts"],
      "theme": ["theme/index.ts"] 
    },
    "resolveJsonModule": true,
    "esModuleInterop": true,
  }
}
